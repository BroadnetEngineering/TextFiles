{
    "name": "rotelloj/textfiles",
    "description": "Broadnet coding challenge.",
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test-unit": "./vendor/bin/phpunit tests --colors=always --filter CSVTest",
        "test-big-data": "./vendor/bin/phpunit tests  --colors=always --filter BigData"
    }
}
